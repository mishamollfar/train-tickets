{
  "name": "railway-ticket",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --aot --source-map --proxy-config proxy.json",
    "hmr": "ng serve --host 0.0.0.0 --hmr --configuration=hmr --proxy-config proxy.json",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "check-format": "prettier --config .prettierrc.yaml --list-different \"src/**/*.ts\"",
    "format": "prettier --config .prettierrc.yaml --write \"{e2e,src,test}/**/*.ts\"",
    "precommit": "node precommit.js && lint-staged",
    "build-prod": "ng build --aot --prod --stats-json --source-map --progress false --build-optimizer",
    "postbuild-prod": "gulp compress",
    "build:ssr": "npm run compile:server && npm run build:client-and-server-bundles && npm run webpack:server",
    "compile:server": "ng run check-ticket-frontend:server",
    "serve:ssr": "node dist/server",
    "build:client-and-server-bundles": "ng build --prod",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
  "lint-staged": {
    "{src,e2e}/**/*.ts": [
      "npm run format",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.3",
    "@angular/cdk": "^7.0.3",
    "@angular/common": "^7.0.3",
    "@angular/compiler": "^7.0.3",
    "@angular/core": "^7.0.3",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "^7.0.3",
    "@angular/http": "^7.0.3",
    "@angular/material": "^7.0.3",
    "@angular/platform-browser": "^7.0.3",
    "@angular/platform-browser-dynamic": "^7.0.3",
    "@angular/platform-server": "^7.0.3",
    "@angular/router": "^7.0.3",
    "@angularclass/hmr": "^2.1.3",
    "@nguniversal/module-map-ngfactory-loader": "^7.0.2",
    "@ngxs/logger-plugin": "^3.2.0",
    "@ngxs/store": "^3.2.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^2.2.0",
    "ngx-toastr": "^9.1.1",
    "rxjs": "^6.3.3",
    "ts-loader": "^5.3.0",
    "zone.js": "~0.8.26",
    "express": "latest"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.5",
    "@angular/compiler-cli": "^7.0.3",
    "@angular/language-service": "^7.0.3",
    "@sentry/browser": "^4.3.0",
    "@types/jasmine": "~2.8.11",
    "@types/jasminewd2": "~2.0.6",
    "@types/node": "~10.12.5",
    "codelyzer": "^4.5.0",
    "gulp": "^3.9.1",
    "gulp-brotli": "^1.2.1",
    "gulp-gzip": "^1.4.2",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^8.0.4",
    "prettier": "1.15.2",
    "protractor": "^5.4.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "webpack-cli": "^3.1.2"
  }
}
